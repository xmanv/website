ode\",[t._v(\"wrapper.vm\")]),t._v(\"进而可以访问到组件所有的\"),n(\"code\",[t._v(\"props\")]),t._v(\"、\"),n(\"code\",[t._v(\"data\")]),t._v(\"和\"),n(\"code\",[t._v(\"methods\")]),t._v(\"等等。\")]),t._v(\" \"),n(\"p\",[n(\"strong\",[t._v(\"注意：\")]),t._v(\" 在我们使用\"),n(\"code\",[t._v(\"mount\")]),t._v(\"或者\"),n(\"code\",[t._v(\"shallowMount\")]),t._v(\"的时候，我们可以期望组件响应几乎所有的\"),n(\"code\",[t._v(\"Vue\")]),t._v(\"生命周期函数，但除非手动调用\"),n(\"code\",[t._v(\"wrapper.destory()\")]),t._v(\"函数，否则组件的\"),n(\"code\",[t._v(\"beforeDestroy()\")]),t._v(\"和\"),n(\"code\",[t._v(\"destroyed()\")]),t._v(\"不会被触发。\")]),t._v(\" \"),n(\"div\",{staticClass:\"language-js extra-class\"},[n(\"pre\",{pre:!0,attrs:{class:\"language-js\"}},[n(\"code\",[n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[t._v(\"import\")]),t._v(\" \"),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[t._v(\"{\")]),t._v(\" shallowMount\"),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[t._v(\",\")]),t._v(\" mount \"),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[t._v(\"}\")]),t._v(\" \"),n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[t._v(\"from\")]),t._v(\" \"),n(\"span\",{pre:!0,attrs:{class:\"token string\"}},[t._v('\"@vue/test-utils\"')]),t._v(\"\\n\"),n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[t._v(\"import\")]),t._v(\" HelloWorld \"),n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[t._v(\"from\")]),t._v(\" \"),n(\"span\",{pre:!0,attrs:{class:\"token string\"}},[t._v('\"@/components/HelloWorld.vue\"')]),t._v(\"\\n\\n\"),n(\"span\",{pre:!0,attrs:{class:\"token function\"}},[t._v(\"describe\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[t._v(\"(\")]),n(\"span\",{pre:!0,attrs:{class:\"token string\"}},[t._v('\"HelloWorld.vue\"')]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[t._v(\",\")]),t._v(\" \"),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[t._v(\"(\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[t._v(\")\")]),t._v(\" \"),n(\"span\